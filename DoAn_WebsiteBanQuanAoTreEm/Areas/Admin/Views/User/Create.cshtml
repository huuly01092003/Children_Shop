        </div>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Tạo" class="btn btn-primary" style="width:150px;" />
            <span class="mx-5">@Html.ActionLink("Back to List", "Index")</span>
        </div>
    </div>
    <div>
       
    </div>
</div>
    }




</div>
