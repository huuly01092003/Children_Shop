@model DoAn_WebsiteBanQuanAoTreEm.Models.LoaiQuanAo
@{
    ViewBag.Title = "Insert";
    Layout = "~/Areas/Admin/Views/Shared/Admin_LayoutPage1.cshtml";
}
<div class="container-md my-4">
    <h2>@ViewBag.Title</h2>
    <form action="/Admin/category/insert" method="post" style="width:600px;" class="mx-2">
        <div class="form-group">
            <input type="hidden" class="form-control" id="Id" name="Id">
        </div>
        <div class="form-group">
            <label for="Name">Tên chủ đề</label>
            <input type="text" class="form-control" id="tenLoai" name="tenLoai">
        </div>
        <button type="submit" class="btn btn-success">Thêm sản phẩm</button>
        <a href="/admin/category/index" class="mx-5">Cancel</a>
    </form>
</div>


